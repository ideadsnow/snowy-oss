{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    "dialog:default",
    "fs:default"
  ],
  "allow": [
    {
      "command": "greet"
    },
    {
      "command": "test_s3_connection"
    },
    {
      "command": "list_buckets"
    },
    {
      "command": "list_objects"
    },
    {
      "command": "delete_objects"
    },
    {
      "command": "get_presigned_url"
    },
    {
      "command": "download_file"
    }
  ]
}
